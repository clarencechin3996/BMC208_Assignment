<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vaccineBatchList"
    android:id="@+id/layout_vaccine_batch">

    <TextView
        android:id="@+id/text_view_healthcare_centre_name"
        android:layout_width="406dp"
        android:layout_height="52dp"
        android:layout_marginTop="32dp"
        android:text="Healthcare Centre: "
        android:textColor="@color/black"
        android:textSize="19sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="5dp" />

    <ListView
        android:id="@+id/list_view_vaccine_batch"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/text_view_healthcare_centre_name"/>

</androidx.constraintlayout.widget.ConstraintLayout>